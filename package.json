{
  "name": "@pulbyte/notionsocial-global",
  "version": "1.8.0",
  "description": "Global code resources for Notionsocial",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "browser": "./dist/browser.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "https://github.com/pulbyte/notionsocial-global"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@pulbyte"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/browser.d.ts",
        "import": "./dist/browser.mjs",
        "require": "./dist/browser.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
      }
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "start": "tsup --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@alshdavid/mime-types": "^2.2.3",
    "@babel/core": "^7.12.0",
    "@google-cloud/storage": "^7.11.2",
    "@notion-stuff/blocks-markdown-parser": "^6.0.0",
    "@notionhq/client": "^2.2.15",
    "@redtea/format-axios-error": "^2.1.1",
    "@types/lodash": "^4.17.7",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.10.4",
    "@types/twitter-text": "^3.1.10",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "firebase-admin": "^12.2.0",
    "lodash": "^4.17.21",
    "markdown-to-txt": "^2.0.1",
    "notion-types": "^6.16.0",
    "open-graph-scraper": "^6.7.2",
    "string-to-unicode-variant": "^1.0.9",
    "ts-node": "^10.9.1",
    "tsup": "^8.1.0",
    "twitter-text": "^3.1.0",
    "typescript": "^5.5.2"
  }
}
