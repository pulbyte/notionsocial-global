    "dotenv": "^16.4.5",
    "firebase-admin": "^12.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tsup": "^8.1.0",
    "typescript": "^5.9.2"
  }
