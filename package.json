    "test:media": "jest tests/media.test.ts",
    "test:chooseMediaFilesToDownload": "jest tests/chooseMediaFilesToDownload.test.ts",
    "ig": "npm i @pulbyte/social-stack-lib@latest",
    "il": "npm i \"./../../Social Stack/library (js)\""
